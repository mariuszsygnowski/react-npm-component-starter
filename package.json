{
  "name": "constructor-labs",
  "version": "0.1.0",
  "description": "A minimal boilerplate for building a react component for npm, including a demo page.",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "npm run lib",
    "dev": "concurrently \"npm run lib:watch\" \"npm run docs\"",
    "lib": "babel src/lib -d lib --copy-files",
    "lib:watch": "babel src/lib -w -d lib --copy-files",
    "docs": "webpack-dev-server --mode development",
    "docs:prod": "webpack --mode production"
  },
  "keywords": [],
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.2",
    "css-loader": "^1.0.1",
    "html-webpack-plugin": "^5.5.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "author": "Mariusz Sygnowski <sygnowski.m@gmail.com>",
  "homepage": "https://github.com/mariuszsygnowski/react-npm-component-starter",
  "repository": {
    "type": "git",
    "url": "git@github.com/mariuszsygnowski/react-npm-component-starter.git"
  }
}
